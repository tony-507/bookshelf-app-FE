{
  "name": "bookshelf-react-express-sqlite-app",
  "version": "1.0.0",
  "description": "FE React bookshelf app",
  "private": false,
  "license": "MIT",
  "browserslist": [
    "last 7 versions",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "main": "src/index.tsx",
  "prox": "http://localhost:5000",
  "scripts": {
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "eject": "react-scripts eject",
    "start-front": "react-scripts start",
    "start-server": "nodemon ../server/server.js --watch ../server/*",
    "dev": "BROWSER=safari concurrently \"npm run start-server\" \"npm run start-front\" --kill-others --kill-others-on-fail",
    "start": "npm run start-front",
    "test": "react-scripts test --env=jsdom"
  },
  "dependencies": {
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.6",
    "@types/react-intl": "^3.0.0",
    "@types/react-router-dom": "^5.3.0",
    "axios": "^0.21.1",
    "concurrently": "5.1.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^4.0.3",
    "typescript": "3.8.3"
  },
  "devDependencies": {
    "nodemon": "2.0.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  }
}
